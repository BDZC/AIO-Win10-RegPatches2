:RegConvert
	cls
	call :HEADER "REG CONVER/SCAN", " "
	echo [S] SCAN REGISTRY
	echo:
	echo [C] CONVERT .REG TO .BAT/XML
	echo:
	echo [B] BACK
	call :FOOTER
	CHOICE /C SCB /N /M "YOUR CHOICE ?:"
	if %errorlevel%==1 goto:StartRegscanner
	if %errorlevel%==2 goto:RegConvert.wtarkan
	if %errorlevel%==3 goto:Tools
:StartRegscanner
	pushd %~dp0
	start regscanner\%vera%\regscanner.exe
	goto:RegConvert
:RegConvert.wtarkan
	cls
	call :HEADER "RegConvert by wtarkan", " "
	echo This will convert REG files to other scripts.
	echo:
	if /i "%output%"=="" set output=bat
	echo Path to Regfile
	echo Default: %regpath%
	echo:
	echo Output Format [bat, bat2, vbs, au3]
	echo Default: %output%
	call :FOOTER
	set /p regpath=Set Reg Path :
	if not defined regpath goto:Tools
	set /p output=Set Output Type :
	cls
	call :HEADER "Converting %regpath%", "to %output%"
	call RegConvert\RegConvert.exe /S=%regpath% /O=%output% /T
	call :FOOTER
	pause	
	goto:Tools
