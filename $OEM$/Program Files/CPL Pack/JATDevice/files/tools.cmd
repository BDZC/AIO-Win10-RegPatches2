:ServerCoreInstall
	pushd "%~dp0"
	cls
	call :HEADER "This option installs the Tool in windir", " "
	reg query "HKLM\Software\Microsoft\Windows\CurrentVersion\Run" | findstr /i "JATD.Script" >nul 2>&1
	if %errorlevel%==0 echo ...tool is already installed. && pause && goto :MenuServerCoreInstall
	echo NOTE: This will move the tool to %windir%\
	echo and create link in AutoStart.
	call :FOOTER
	CHOICE /C YN /M "Proceed..[Y/N].?:"
	if %errorlevel%==1 goto :proceed
    if %errorlevel%==2 goto:Tools
:proceed
	echo:
	echo:
	CD /D "%~dp0"
	xcopy "%~dp0..\*.*" /s /q "%windir%\JATD.Script\" /y
	REG ADD HKLM\Software\Microsoft\Windows\CurrentVersion\Run /v JATD.Script /t REG_SZ /d "%windir%\JATD.Script\JATDevice.cmd" /f
	call :FOOTER
	pause
	goto:Tools
:++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++	
::++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
:ServerCoreInstallDelete
	pushd "%~dp0"
	cls
	call :HEADER "This option uninstalls the Tool", " "
	reg query "HKLM\Software\Microsoft\Windows\CurrentVersion\Run" | findstr /i "JATD.Script" >nul 2>&1
	if %errorlevel%==1 (
		echo ...tool is not installed.
		call :FOOTER
		pause
		goto:Tools
	)
	echo NOTE: This will uninstall the tool from %windir%
	echo and delete it from Autostart
	call :FOOTER
	CHOICE /C YN /M "Proceed..[Y/N].?:"
	if %errorlevel%==1 goto :proceedUninstall
    if %errorlevel%==2 goto:Tools
:proceedUninstall
	call :FOOTER
	REG DELETE HKLM\Software\Microsoft\Windows\CurrentVersion\Run /v JATD.Script /f
	if exist "%windir%\JATD.Script" rd /s /q "%windir%\JATD.Script\"
	pause
	goto:Tools
